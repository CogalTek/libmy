#include "libmy.h"

void		__libmy_qsort(t_clist_elm *start, t_clist_elm *end, t_elm_cmp cmp_fct)
{
  void		*pivot;
  t_clist_elm	*wall;
  t_clist_elm	*cur;

  if (start->next == end)
    return;
  pivot = start->ptr;
  wall = start;
  cur = start;
  while (cur)
    {
      
      cur = CLIST_NEXT(end, cur);
    }
  __libmy_qsort(start, wall);
  __libmy_qsort(wall, end);
}

void	clist_qsort(t_clist *list, t_elm_cmp cmp_fct)
{
  if (list)
    __libmy_qsort(list, list, cmp_fct);
}
